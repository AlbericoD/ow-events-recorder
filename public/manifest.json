{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Overwolf ERP",
    "description": "Overwolf Event Recorder/Player",
    "version": "1.0.1",
    "minimum-overwolf-version": "0.165.0",
    "author": "Overwolf",
    "icon": "icons/dock.png",
    "icon_gray": "icons/dock-gray.png",
    "launcher_icon": "icons/launcher.ico",
    "window_icon": "icons/dock.png"
  },
  "permissions": [
    "Extensions",
    "Streaming",
    "GameInfo",
    "Profile",
    "Hotkeys",
    "DesktopStreaming",
    "FileSystem",
    "Clipboard"
  ],
  "data": {
    "extra-objects": {
      "erp": {
        "file": "plugins/erp.dll",
        "class": "overwolf.plugins.erp.ERPPlugin"
      }
    },
    "force_browser": "user",
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "is_background_page": true,
        "background_optimization": false,
        "allow_local_file_access": true
      },
      "main": {
        "file": "main.html",
        "transparent": true,
        "resizable": false,
        "desktop_only": true,
        "native_window": true,
        "show_in_taskbar": true,
        "keep_window_location": true,
        "override_on_update": true,
        "size": {
          "width": 800,
          "height": 560
        }
      }
    },
